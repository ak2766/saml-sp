<div class="col-md-6 col-md-offset-3">
	<div class="panel panel-default">
	  <div class="panel-heading">
	    <h3 class="panel-title">SSO User</h3>
	  </div>
	  <div class="panel-body">
	  	<form class="form-horizontal">
				<div class="form-group">
				  <label for="userName" class="col-sm-3 control-label">Subject Name ID</label>
				  <div class="col-sm-9">
				    <p id="userName" class="form-control-static">{{user.nameID}}</p>
				  </div>
				</div>

				<div class="form-group">
				  <label for="firstName" class="col-sm-3 control-label">First Name</label>
				  <div class="col-sm-9">
				    <p id="firstName" class="form-control-static">{{user.firstName}}</p>
				  </div>
				</div>

				<div class="form-group">
				  <label for="firstName" class="col-md-3 control-label">Last Name</label>
				  <div class="col-sm-9">
				    <p id="lastName" class="form-control-static">{{user.lastName}}</p>
				  </div>
				</div>

				<div class="form-group">
				  <label for="firstName" class="col-md-3 control-label">Email</label>
				  <div class="col-md-9">
				    <p id="email" class="form-control-static">{{user.email}}</p>
				  </div>
				</div>
			</form>
	  </div>
	</div>
</div>


